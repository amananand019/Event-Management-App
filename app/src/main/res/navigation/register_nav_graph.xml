<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/adminRegisterTabFragment">
    <fragment
        android:id="@+id/adminRegisterTabFragment"
        android:name="com.devil.premises.unicorn.AdminRegisterTabFragment"
        android:label="AdminRegisterTabFragment" >
        <action
            android:id="@+id/action_adminRegisterTabFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/studentRegisterTabFragment"
        android:name="com.devil.premises.unicorn.StudentRegisterTabFragment"
        android:label="StudentRegisterTabFragment" >
        <action
            android:id="@+id/action_studentRegisterTabFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.devil.premises.unicorn.RegisterActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_register" />
</navigation>